{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "events": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event.event",
      "mappedBy": "user"
    },
    "messages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::message.message",
      "mappedBy": "usersPermissionsUser"
    },
    "userOrganizations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::organization.organization",
      "inversedBy": "users"
    },
    "adminOrganizations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::organization.organization",
      "inversedBy": "adminUsers"
    },
    "userProjects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project.project",
      "inversedBy": "users"
    },
    "login_at": {
      "type": "datetime"
    },
    "type": {
      "type": "string"
    },
    "widget": {
      "type": "richtext"
    },
    "phone": {
      "type": "string"
    },
    "verificationCode": {
      "type": "string"
    },
    "userOrganizationRoles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-organization-role.user-organization-role",
      "mappedBy": "user"
    }
  }
}
